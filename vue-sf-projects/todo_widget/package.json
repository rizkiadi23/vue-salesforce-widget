{
  "name": "todo",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build",
    "update_project": "vue-cli-service build && rm -rf ../../force-app/main/default/staticresources/SfVueTodoWidget/* && cp -r dist/* -t ../../force-app/main/default/staticresources/SfVueTodoWidget/",
    "lint": "vue-cli-service lint"
  },
  "dependencies": {
    "@salesforce-ux/design-system": "^2.9.4",
    "core-js": "^2.6.5",
    "dotenv": "^8.0.0",
    "firebase": "^6.1.1",
    "vue": "^2.6.10",
    "vuex": "^3.1.1"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "^5.0.8",
    "@vue/cli-plugin-eslint": "^3.8.0",
    "@vue/cli-service": "^5.0.8",
    "babel-eslint": "^10.0.1",
    "eslint": "^5.16.0",
    "eslint-plugin-vue": "^5.0.0",
    "vue-template-compiler": "^2.6.10"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/essential",
      "eslint:recommended"
    ],
    "rules": {},
    "parserOptions": {
      "parser": "babel-eslint"
    }
  },
  "postcss": {
